modules = ["nodejs-20", "web", "python-3.11", "postgresql-16"]
run = "npm run dev"

[nix]
channel = "stable-25_05"

[deployment]
run = ["bash", "-c", "node api/server.js"]
deploymentTarget = "vm"
build = ["npm", "run", "build"]

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Backend"

[[workflows.workflow]]
name = "App"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Backend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node api/server.js"
waitForPort = 3001

[workflows.workflow.metadata]
outputType = "console"

[auth]
pageEnabled = false
buttonEnabled = false

